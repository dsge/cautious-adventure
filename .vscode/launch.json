    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "/home/geri/workspace/godot/bin/godot.linuxbsd.opt.tools.64",
            "args": [
                "--path",
                "${workspaceFolder}",
                "--verbose"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "linuxbuild"
        },
        {
            "name": "Open project in Godot Editor",
